    "jstat": "~1.9.6",
    "@stdlib/stats-base-dists-t-quantile": "0.2.1",
    "array-normalize": "~2.0.0",
    "bayesian-changepoint": "~1.0.1",
    "density-clustering": "~1.3.0",
